# user-specific mailcap

application/msword; libreoffice --nologo --writer '%s'; edit=libreoffice --nologo --writer '%s'; test=test -n "$DISPLAY"; description="Microsoft Word Document"; nametemplate=%s.doc
application/msword; antiword -tf '%s'; copiousoutput; description="Microsoft Word Document"; nametemplate=%s.doc
application/pdf; zathura '%s'; test=test -n "$DISPLAY"; description="PDF Document"; nametemplate=%s.pdf
application/pdf; pdftotext '%s' -; copiousoutput; description="PDF Document"; nametemplate=%s.pdf

image/bmp; display '%s'; edit=gimp '%s'; test=test -n "$DISPLAY"; description="BMP Image"; nametemplate=%s.bmp
image/gif; display '%s'; edit=gimp '%s'; test=test -n "$DISPLAY"; description="GIF Image"; nametemplate=%s.gif
image/jpg; display '%s'; edit=gimp '%s'; test=test -n "$DISPLAY"; description="JPG Image"; nametemplate=%s.jpg
image/jpg; jp2a '%s'; copiousoutput; description="JPG Image"; nametemplate=%s.jpg
image/jpeg; display '%s'; edit=gimp '%s'; test=test -n "$DISPLAY"; description="JPEG Image"; nametemplate=%s.jpeg
image/jpeg; jp2a '%s'; copiousoutput; description="JPEG Image"; nametemplate=%s.jpeg
image/png; display '%s'; edit=gimp '%s'; test=test -n "$DISPLAY"; description="PNG Image"; nametemplate=%s.png

text/html; vimb '%s'; test=test -n "$DISPLAY"; description="HTML Document"; nametemplate=%s.html

