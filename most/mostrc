% Rebind for the b√©po layout
unsetkey    s
setkey      "up"        s

unsetkey    t
setkey      "down"      t

unsetkey    g
setkey      "bob"       gg

unsetkey    G
setkey      "eob"       G

unsetkey    i
setkey      "page_left" i

unsetkey    e
setkey      "page_right"    e


